<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Comment</key>
		<string>Intel HD4000 1920x1080 on Z77X chipset</string>
		<key>Boot</key>
		<dict>
			<key>Arguments</key>
			<string></string>
			<key>DefaultLoader</key>
			<string></string>
			<key>DefaultVolume</key>
			<string>LastBootedVolume</string>
			<key>NeverHibernate</key>
			<true/>
			<key>Fast</key>
			<false/>
			<key>Timeout</key>
			<integer>5</integer>
			<key>Secure</key>
			<false/>
			<key>CustomLogo</key>
			<false/>
		</dict>
		<key>GUI</key>
		<dict>
			<key>Custom</key>
			<dict>
				<key>Entries</key>
				<array>
					<dict>
						<key>AddArguments</key>
						<string></string>
						<key>Arguments</key>
						<string>initrd=/intel-ucode.img initrd=/initramfs-linux.img root=PARTUUID=XXXXXXXXXXXXXXXX rw quiet</string>
						<key>Hidden</key>
						<false/>
						<key>Ignore</key>
						<false/>
						<key>Path</key>
						<string>\vmlinuz-linux</string>
						<key>FullTitle</key>
						<string>Linux</string>
						<key>Type</key>
						<string>Linux/LinuxKernel</string>
						<key>Volume</key>
						<string>PARTUUID EFI</string>
					</dict>
				</array>
			</dict>
			<key>CustomIcons</key>
			<false/>
			<key>Hide</key>
			<array>
				<string>BOOTX64.EFI</string>
			</array>
			<key>Language</key>
			<string>en:0</string>
			<key>Mouse</key>
			<dict>
				<key>Enabled</key>
				<false/>
			</dict>
			<key>Scan</key>
			<dict>
				<key>Entries</key>
				<true/>
				<key>Legacy</key>
				<false/>
				<key>Kernel</key>
				<string>Newest</string>
			</dict>
			<key>ScreenResolution</key>
			<string>1920x1080</string>
			<key>Theme</key>
			<string></string>
			<key>TextOnly</key>
			<false/>
		</dict>
	</dict>
</plist>